{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tsbuildinfo",
    "baseUrl": "./src",
    "rootDir": "./src",
    "outDir": "./dist",
    "lib": [
      "ESNext"
    ],
    "noImplicitAny": false
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "**/*spec.ts",
    "src/commands/disabled/**/*",
    "src/commands/*-enhanced.ts"
  ]
}